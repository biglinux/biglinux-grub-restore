# 
# File: biglinux-grub-restore/usr/share/biglinux/grub-restore/gui/application.py, line: 58
msgid "A tool to restore the GRUB bootloader."
msgstr "Інструмент для відновлення завантажувача GRUB."
# 
# File: biglinux-grub-restore/usr/share/biglinux/grub-restore/gui/window.py, line: 53
msgid "About"
