# 
# File: biglinux-grub-restore/usr/share/biglinux/grub-restore/gui/application.py, line: 58
msgid "A tool to restore the GRUB bootloader."
msgstr "Ein Werkzeug zur Wiederherstellung des GRUB-Bootloaders."
# 
# File: usr/share/biglinux/grub-restore/gui/window.py, line: 53
msgid "About"
msgstr "Über"
# 
# File: usr/share/biglinux/grub-restore/gui/window.py, line: 54
